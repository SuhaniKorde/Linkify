    <span class="hljs-keyword">const</span> fileUrl = <span class="hljs-string">`<span class="hljs-subst">${req.protocol}</span>://<span class="hljs-subst">${req.get(<span class="hljs-string">"host"</span>)}</span>/<span class="hljs-subst">${req.file.filename}</span>`</span>;
    res.<span class="hljs-title function_">status</span>(<span class="hljs-number">200</span>).<span class="hljs-title function_">json</span>({ fileUrl });
});
