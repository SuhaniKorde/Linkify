<span class="hljs-keyword">function</span> <span class="hljs-title function_">App</span>(<span class="hljs-params"></span>) {
  <span class="hljs-keyword">const</span> [file, setFile] = <span class="hljs-title function_">useState</span>(<span class="hljs-literal">null</span>);
  <span class="hljs-keyword">const</span> [downloadLink, setDownloadLink] = <span class="hljs-title function_">useState</span>(<span class="hljs-string">''</span>);

