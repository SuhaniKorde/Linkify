    }

    <span class="hljs-comment">// Use a fixed public IP address or domain for generating file URL</span>
    <span class="hljs-keyword">const</span> fileUrl = <span class="hljs-string">`http://&lt;your-public-ip-or-domain&gt;:5000/files/<span class="hljs-subst">${req.file.id}</span>`</span>;
    res.<span class="hljs-title function_">status</span>(<span class="hljs-number">200</span>).<span class="hljs-title function_">json</span>({ fileUrl });
});

<span class="hljs-comment">// Route to download file by ID</span>
app.<span class="hljs-title function_">get</span>(<span class="hljs-string">"/files/:id"</span>, <span class="hljs-keyword">async</span> (req, res) =&gt; {
